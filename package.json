{
  "name": "boiler",
  "description": "AIRLIFT Boiler",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.16",
    "@fortawesome/free-brands-svg-icons": "^5.8.0",
    "@fortawesome/pro-light-svg-icons": "^5.8.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxt/typescript": "^2.4.5",
    "@nuxtjs/axios": "5.4.1",
    "@nuxtjs/component-cache": "^1.1.4",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/google-tag-manager": "^2.1.4",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/sentry": "^2.3.1",
    "@nuxtjs/sitemap": "^0.2.2",
    "@storybook/vue": "^5.0.3",
    "core-js": "^3.0.0",
    "nuxt-buefy": "^0.3.4",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-property-decorator": "2.1.3",
    "nuxt-ts-edge": "latest",
    "vue": "^2.6.10",
    "vue-fontawesome": "^0.0.2",
    "vuex": "^3.1.0",
    "vuex-class": "0.3.2"
  },
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "dev": "nuxt-ts dev",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "tsc": "tsc --showConfig",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "testc": "tsc -p tsconfig.json",
    "lint": "tslint --project .",
    "lintfix": "tslint --fix --project .",
    "post-update": "yarn upgrade --latest",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "storybook": "start-storybook"
  },
  "devDependencies": {
    "@nuxt/vue-app": "2.4.5",
    "@storybook/addon-info": "^5.0.3",
    "@storybook/cli": "^5.0.3",
    "@types/jest": "^24.0.11",
    "@types/moment": "^2.13.0",
    "@types/node": "11.11.4",
    "@types/storybook__vue": "^5.0.0",
    "@types/webpack-env": "1.13.9",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-preset-vue": "^2.0.2",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^1.16.4",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.4000",
    "typescript-tslint-plugin": "^0.3.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "coverageDirectory": "<rootDir>/tests/coverage"
  }
}
